{
  "name": "react-pure-modal",
  "version": "3.0.3",
  "description": "React modal",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "sideEffects": [
    "*.css",
    "*.module.css",
    "**/*.css"
  ],
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Memcrab/react-pure-modal.git"
  },
  "keywords": [
    "react",
    "modal",
    "dialog"
  ],
  "author": "Max Mykhailenko",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Memcrab/react-pure-modal/issues"
  },
  "homepage": "https://github.com/Memcrab/react-pure-modal#readme",
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "typecheck": "tsc -p tsconfig.types.json",
    "storybook": "npx storybook dev",
    "storybook:build": "storybook build -o storybook-static",
    "storybook:deploy": "npm run storybook:build"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.5",
    "@rsbuild/core": "1.5.13",
    "@rsbuild/plugin-react": "^1.4.1",
    "@rslib/core": "^0.15.0",
    "@storybook/addon-docs": "^10.1.0",
    "@storybook/addon-onboarding": "^10.1.0",
    "@storybook/react": "^10.1.0",
    "@types/react": "^19.1.15",
    "@types/react-dom": "19.2.0",
    "react": "^19.1.1",
    "storybook": "^10.1.0",
    "storybook-addon-rslib": "^3.0.0",
    "storybook-react-rsbuild": "^3.0.0",
    "typescript": "^5.9.2",
    "typescript-plugin-css-modules": "5.2.0"
  },
  "peerDependencies": {
    "react": "^18.0 || ^19.0",
    "react-dom": "^18.0 || ^19.0"
  }
}
